
test: (output expected) {
    output.is(expected).then('PASS' 'FAIL').print()
}

'Testing Simple Tables, Methods, and Assignment'.print()
chain1: (x) { x.+(2).is(3) }
chain2: (x) { x.+(2).*(3).sqrt() }
test(chain1(1) true)
test(chain1(2) false)
test(chain2(1) 3)
