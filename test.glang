=('pass' '[ PASS ]')
=('fail' '[ FAIL ]')

=('it' (message fun input test) {
    print(message)
    =('result' test(fun(input)))
    print(result.is(true).then(pass fail))
})

=('math' (x) {
    return(x.+(1).*(2).squared())
})
it('Should return 144' math 5 (result){ return(result.is(144)) })

=('checkName' (name) {
    return(name.is('Gerb'))    
})
it('Should return true' checkName 'Gerba' (result){ return(result.is(true)) })
it('Should return false' checkName 'Rawren' (result){ return(result.is(false)) })

=('hiddenArg' (){
    return(x)
})
hiddenArg.=('x' 'It worked!')
it('Should return "It worked!"' hiddenArg '' (result){ return(result.is('It worked!'))})