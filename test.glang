=('pass' '[ PASS ]')
=('fail' '[ FAIL ]')

=('math' (x) {
    return(x.+(1).*(2).squared())
})

print('Following should be 144: ')
print(math(5))
print(math(5).is(144).then(pass fail))

print('\n')

set('checkName' (name) {
    return(name.is('Gerb'))    
})

print('Following should be true, followed by false:')
=('out1' checkName('Gerb'))
=('out2' checkName('Rawren'))
print(out1)
print(out2)
print(out1.and(out2.not()).then(pass fail))

print('\n')